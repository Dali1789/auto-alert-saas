# Auto-Alert SaaS Environment Variables
# Copy this file to .env and fill in your actual values

# Server Configuration
NODE_ENV=production
PORT=3001
FRONTEND_URL=https://auto-alert.vercel.app

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Retell AI Configuration
RETELL_API_KEY=key_your-retell-api-key
RETELL_AGENT_ID=your-retell-agent-id
RETELL_PHONE_NUMBER=+4915123456789

# Resend Email Configuration
RESEND_API_KEY=re_your-resend-api-key

# Webhook Security
WEBHOOK_SECRET=your-secure-webhook-secret-min-32-chars

# Optional: SMS Configuration (for future implementation)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+4915987654321

# Optional: Monitoring & Analytics
SENTRY_DSN=your-sentry-dsn-for-error-tracking