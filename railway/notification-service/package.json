{
  "name": "auto-alert-notification-service",
  "version": "1.0.0",
  "description": "Notification service with Retell AI voice calls and Resend emails",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:security": "jest tests/security.test.js",
    "test:watch": "jest --watch",
    "security:validate": "node ../scripts/security-validation.js",
    "security:audit": "npm audit && npm outdated",
    "security:fix": "npm audit fix",
    "prestart": "node ../scripts/security-validation.js",
    "postinstall": "npm audit --audit-level moderate"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "@supabase/supabase-js": "^2.39.0",
    "resend": "^3.2.0",
    "axios": "^1.6.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "node-fetch": "^2.7.0",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.11.0",
    "express-winston": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@types/jest": "^29.5.8",
    "jest-environment-node": "^29.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "notification",
    "voice-calls",
    "retell-ai",
    "auto-alert",
    "railway"
  ],
  "author": "Dali1789",
  "license": "MIT"
}